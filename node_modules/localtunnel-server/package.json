{
  "author": "KhoaZero123 <khoazero123@gmail.com>",
  "name": "localtunnel-server",
  "description": "expose localhost to the world",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/khoazero123/localtunnel-server.git"
  },
  "dependencies": {
    "book": "1.3.3",
    "debug": "3.1.0",
    "esm": "3.0.34",
    "human-readable-ids": "1.0.3",
    "koa": "2.5.1",
    "koa-router": "7.4.0",
    "koa-static": "5.0.0",
    "localenv": "0.2.2",
    "markdown-it": "8.4.2",
    "optimist": "0.6.1",
    "pump": "3.0.0",
    "querystring": "0.2.0",
    "tldjs": "2.3.1"
  },
  "devDependencies": {
    "cross-env": "5.2.0",
    "mocha": "5.1.1",
    "nodemon": "1.18.9",
    "supertest": "3.1.0",
    "ws": "5.1.1"
  },
  "scripts": {
    "test": "mocha --check-leaks --require esm './**/*.test.js'",
    "start": "./bin/server",
    "dev": "cross-env DEBUG='localtunnel,localtunnel:*,lt:*' nodemon -r esm --inspect bin/server --domain localhost.tld:3000 --port 3000"
  },
  "nodemonConfig": {
    "watch": [
      ".env",
      "server.js",
      "lib"
    ]
  }
}
